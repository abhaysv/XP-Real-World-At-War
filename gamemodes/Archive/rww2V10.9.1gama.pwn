stock GetPlayerIDbyIP(const ip[])
{
	new pip[16];
	for (new i = 0; i < GetMaxPlayers(); i ++)
	{
		if(!IsPlayerConnected(i)) continue;
		GetPlayerIp(i, pip, 16);
	    if (!strcmp(ip, pip, true))
		{
			return i;
		}
	}
	return -1;
}

